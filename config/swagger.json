                },
                "lastName":{
                   "type":"string"
                },
                "phone":{
                   "type":"string"
                },
                "gender":{
                   "type":"string"
                },
                "country":{
                   "type":"string"
                }
             },
             "xml":{
                "name":"Updateprofile"
             }
          }
